{
  "name": "@vcl/material-style-inputs",
  "description": "Material style inputs",
  "version": "0.6.0",
  "main": "package.json",
  "scripts": {
    "test": "vcl-preprocessor ./index.sss ./build/test.css",
    "browsersync": "browser-sync start --server --files \"demo\" \"build\" --index \"/build/index.html\" ",
    "build-demo": "vcl-build-demo --watch --vcl-root=..",
    "start": "npm-run-all --parallel build-demo browsersync"
  },
  "style": "index.sss",
  "demoStyle": "demo.sss",
  "author": {
    "name": "n-fuse GmbH",
    "email": "info@n-fuse.co",
    "url": "https://www.n-fuse.co"
  },
  "license": "MIT",
  "devDependencies": {
    "@vcl/build-demo": "^0.6.0",
    "@vcl/preprocessor": "^0.6.0",
    "browser-sync": "^2.26.3",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@vcl/form-control-label": "^0.6.0",
    "@vcl/input": "^0.6.0",
    "@vcl/theme": "^0.6.0"
  },
  "keywords": [
    "css",
    "vcl",
    "component",
    "input",
    "material"
  ],
  "vcl": {
    "categories": [
      {
        "title": "Form Controls",
        "itemPriority": 25,
        "priority": 70
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
