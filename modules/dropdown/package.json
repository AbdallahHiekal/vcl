    "@vcl/build-demo": "^0.4.2",
    "@vcl/font-awesome": "^0.4.2",
    "@vcl/icogram": "^0.4.2",
    "@vcl/icon": "^0.4.2",
    "@vcl/preprocessor": "^0.4.2",
    "@vcl/typography": "^0.4.2",
    "browser-sync": "^2.26.7",
    "npm-run-all": "^4.1.5",
