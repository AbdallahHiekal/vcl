{
  "name": "@vcl/bw-theme",
  "description": "The VCL BW theme",
  "version": "0.2.0",
  "main": "package.json",
  "scripts": {
    "test": "VCL_NO_PIPE=1 vcl-preprocessor --verbose ./package.json ./build/test.css",
    "start": "node ./node_modules/gulp/bin/gulp dev"
  },
  "style": "index.styl",
  "author": {
    "name": "n-fuse GmbH",
    "email": "info@n-fuse.de",
    "url": "www.n-fuse.de"
  },
  "contributors": [
    {
      "name": "Thomas Hoppe",
      "email": "thomas.hoppe@n-fuse.de"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@vcl/gulp-vcl-preprocessor": "^0.2.1",
    "pre-commit": "^1.1.2",
    "@vcl/preprocessor": "^0.2.2",
    "@vcl/typography": "^0.2.0"
  },
  "dependencies": {
    "@vcl/default-theme": "^0.2.0",
    "@vcl/default-theme-terms": "^0.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vcl/bw-theme.git"
  },
  "keywords": [
    "css",
    "vcl",
    "component",
    "default",
    "theme"
  ],
  "vcl": {
    "needs": [
      "theme"
    ],
    "provides": [],
    "overwrites": [
      "theme"
    ],
    "categories": [
      {
        "title": "Themes",
        "itemPriority": 30,
        "priority": 180
      }
    ]
  }
}
