{
  "name": "@vcl/typography",
  "description": "Styling for basic typographical elements",
  "version": "0.3.0",
  "main": "package.json",
  "scripts": {
    "test": "VCL_NO_PIPE=1 vcl-preprocessor --verbose ./package.json ./build/test.css",
    "browsersync": "node_modules/.bin/browser-sync start --server --files \"demo\" \"build\" --index \"/build/index.html\" ",
    "build-demo": "node node_modules/@vcl/build-demo/index.js",
    "start": "npm run browsersync & npm run build-demo"
  },
  "style": "index.sss",
  "author": {
    "name": "n-fuse GmbH",
    "email": "info@n-fuse.co",
    "url": "www.n-fuse.co"
  },
  "contributors": [
    {
      "name": "Thomas Hoppe",
      "email": "thomas.hoppe@n-fuse.co"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@vcl/build-demo": "^0.4.0-3",
    "@vcl/gulp-vcl-preprocessor": "^0.2.3",
    "@vcl/preprocessor": "^0.3.0-2",
    "gulp": "^4.0.0",
    "pre-commit": "^1.2.2",
    "browser-sync": "^2.26.3",
    "@vcl/icogram": "^0.3.0",
    "@vcl/icon": "^0.3.0",
    "@vcl/font-awesome": "^0.3.0"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "@vcl/theme": "^0.3.0-1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vcl/typography.git"
  },
  "keywords": [
    "css",
    "vcl",
    "component",
    "typography"
  ],
  "vcl": {
    "categories": [
      {
        "title": "Typography",
        "itemPriority": 10,
        "priority": 20
      }
    ]
  }
