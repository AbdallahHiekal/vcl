{
  "name": "@vcl/material-design-icons",
  "description": "Integration with the material design icons",
  "version": "0.2.6",
  "main": "package.json",
  "scripts": {
    "test": "VCL_NO_PIPE=1 vcl-preprocessor --verbose ./package.json ./build/test.css",
    "browsersync": "node_modules/.bin/browser-sync start --server --files \"demo\" \"build\" --index \"/build/index.html\" ",
    "build-demo": "node node_modules/@vcl/build-demo/index.js",
    "start": "npm run browsersync & npm run build-demo"
  },
  "style": "index.sss",
  "author": {
    "name": "n-fuse GmbH",
    "email": "info@n-fuse.co",
    "url": "www.n-fuse.co"
  },
  "contributors": [
    {
      "name": "Thomas Hoppe",
      "email": "thomas.hoppe@n-fuse.co"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@vcl/build-demo": "^0.4.0-3",
    "@vcl/preprocessor": "^0.3.0-2",
    "browser-sync": "2.26.3",
    "pre-commit": "1.2.2"
  },
  "dependencies": {
    "normalize.css": "8.0.0",
    "@vcl/theme": "^0.3.0-1",
    "@vcl/typography": "0.2.4",
    "@mdi/font": "3.0.39"
  },
  "keywords": [
    "css",
    "vcl",
    "component",
    "font",
    "awesome"
  ],
  "vcl": {
    "needs": [
      "theme"
    ],
    "provides": [],
    "categories": [
      {
        "title": "Icons",
        "itemPriority": 50,
        "priority": 140
      }
    ]
  }
}
