{
  "name": "@vcl/app-footer",
  "description": "The area on the bottom of an app, usually containing links and content info",
  "version": "0.2.3",
  "main": "package.json",
  "scripts": {
    "test": "VCL_NO_PIPE=1 vcl-preprocessor --verbose ./package.json ./build/test.css",
    "browsersync": "node_modules/.bin/browser-sync start --server --files \"demo\" \"build\" --index \"/build/index.html\" ",
    "build-demo": "node node_modules/@vcl/build-demo/index.js",
    "start": "npm run build-demo & npm run browsersync"
  },
  "style": "index.sss",
  "author": {
    "name": "n-fuse GmbH",
    "email": "info@n-fuse.co",
    "url": "www.n-fuse.co"
  },
  "contributors": [
    {
      "name": "Thomas Hoppe",
      "email": "thomas.hoppe@n-fuse.co"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "pre-commit": "^1.1.2",
    "@vcl/build-demo": "^0.4.0-2",
    "@vcl/default-theme": "^0.2.0",
    "@vcl/default-theme-terms": "^0.2.0",
    "@vcl/preprocessor": "^0.3.0-2",
    "browser-sync": "^2.24.7"
  },
  "dependencies": {
    "@vcl/typography": "^0.2.0",
    "@vcl/theme": "^0.3.0-1",
    "@vcl/list": "^0.2.1"
  },
  "keywords": [
    "css",
    "vcl",
    "component",
    "application",
    "footer"
  ],
  "vcl": {
    "needs": [
      "theme"
    ],
    "provides": [
      "app-area-footer"
    ],
    "categories": [
      {
        "title": "Areas",
        "itemPriority": 40,
        "priority": 30
      }
    ]
  }
}
