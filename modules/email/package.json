{
  "name": "@vcl/email",
  "description": "Basic HTML email styling",
  "version": "0.2.6",
  "main": "package.json",
  "scripts": {
    "test": "VCL_NO_PIPE=1 vcl-preprocessor --verbose ./package.json ./build/test.css",
    "browsersync": "node_modules/.bin/browser-sync start --server --files \"demo\" \"build\" --index \"/build/index.html\" ",
    "build-demo": "node node_modules/@vcl/build-demo/index.js",
    "democss": "node_modules/.bin/ vcl-preprocessor demo.sss demo/demo.css",
    "start": "npm run browsersync & npm run build-demo"
  },
  "style": "index.sss",
  "author": {
    "name": "n-fuse GmbH",
    "email": "info@n-fuse.co",
    "url": "www.n-fuse.co"
  },
  "contributors": [
    {
      "name": "Thomas Hoppe",
      "email": "thomas.hoppe@n-fuse.co"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@vcl/build-demo": "^0.4.0-2",
    "@vcl/default-theme": "0.2.4",
    "@vcl/default-theme-terms": "0.2.5",
    "@vcl/layout-spans": "0.2.1",
    "@vcl/list": "0.2.3",
    "@vcl/print": "0.2.1",
    "@vcl/size-modulation": "0.2.3",
    "@vcl/sum-table": "0.2.1",
    "@vcl/table": "0.2.3",
    "@vcl/typography": "0.2.4",
    "@vcl/utils": "0.2.1",
    "browser-sync": "^2.24.7",
    "normalize.css": "8.0.1",
    "pre-commit": "^1.1.2"
  },
  "dependencies": {},
  "keywords": [
    "css",
    "vcl",
    "component",
    "email"
  ],
  "vcl": {
    "needs": [
      "theme"
    ],
    "provides": [],
    "categories": [
      {
        "title": "Email",
        "itemPriority": 10,
        "priority": 200
      }
    ]
  }
}
