{
  "name": "@vcl/spinner",
  "description": "Spinner based input for integer values",
  "version": "0.2.3",
  "main": "package.json",
  "scripts": {
    "test": "VCL_NO_PIPE=1 vcl-preprocessor --verbose ./package.json ./build/test.css",
    "browsersync": "node_modules/.bin/browser-sync start --server --files \"demo\" \"build\" --index \"/build/index.html\" ",
    "build-demo": "node node_modules/@vcl/build-demo/index.js",
    "start": "npm run browsersync & npm run build-demo"
  },
  "style": "index.styl",
  "author": {
    "name": "n-fuse GmbH",
    "email": "info@n-fuse.co",
    "url": "www.n-fuse.co"
  },
  "contributors": [
    {
      "name": "Thomas Hoppe",
      "email": "thomas.hoppe@n-fuse.co"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@vcl/build-demo": "^0.3.1",
    "@vcl/button": "^0.2.0",
    "@vcl/button-group": "^0.2.0",
    "@vcl/default-theme": "^0.2.0",
    "@vcl/default-theme-terms": "^0.2.0",
    "@vcl/flex-layout": "^0.2.0",
    "@vcl/font-awesome": "^0.2.0",
    "@vcl/icogram": "^0.2.0",
    "@vcl/icon": "^0.2.0",
    "@vcl/input": "^0.2.0",
    "@vcl/input-group": "^0.2.6",
    "@vcl/preprocessor": "^0.2.2",
    "@vcl/typography": "^0.2.0",
    "normalize.css": "^7.0.0",
    "pre-commit": "^1.1.2",
    "browser-sync": "^2.24.7"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/vcl/spinner.git"
  },
  "keywords": [
    "css",
    "vcl",
    "component",
    "spinner"
  ],
  "vcl": {
    "needs": [
      "theme"
    ],
    "provides": [
      "spinner"
    ],
    "categories": [
      {
        "title": "Form Controls",
        "itemPriority": 80,
        "priority": 70
      }
    ]
  }
}
