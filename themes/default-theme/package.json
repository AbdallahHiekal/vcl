{
  "name": "@vcl/default-theme",
  "description": "The VCL default theme for the core modules collection",
  "version": "0.2.3",
  "main": "package.json",
  "scripts": {
    "test": "VCL_NO_PIPE=1 vcl-preprocessor --verbose ./package.json ./build/test.css",
    "start": "node ./node_modules/gulp/bin/gulp dev"
  },
  "style": "index.styl",
  "author": {
    "name": "n-fuse GmbH",
    "email": "info@n-fuse.co",
    "url": "www.n-fuse.co"
  },
  "contributors": [
    {
      "name": "Thomas Hoppe",
      "email": "thomas.hoppe@n-fuse.co"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@vcl/default-theme-terms": "^0.2.2",
    "@vcl/gulp-vcl-preprocessor": "^0.2.1",
    "@vcl/preprocessor": "^0.2.4",
    "@vcl/typography": "^0.2.2",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {},
  "keywords": [
    "css",
    "vcl",
    "component",
    "default",
    "theme"
  ],
  "vcl": {
    "needs": [
      "theme-terms"
    ],
    "provides": [
      "theme"
    ],
    "categories": [
      {
        "title": "Themes",
        "itemPriority": 10,
        "priority": 180
      }
    ]
  }
}
