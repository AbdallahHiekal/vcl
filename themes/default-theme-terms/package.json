{
  "name": "@vcl/default-theme-terms",
  "description": "The terms of the VCL default theme",
  "version": "0.2.5",
  "main": "package.json",
  "scripts": {
    "test": "VCL_NO_PIPE=1 vcl-preprocessor --verbose ./package.json ./build/test.css",
    "browsersync": "node_modules/.bin/browser-sync start --server --files \"demo\" \"build\" --index \"/build/index.html\" ",
    "build-demo": "node node_modules/@vcl/build-demo/index.js",
    "democss": "node_modules/.bin/ vcl-preprocessor demo.sss demo/demo.css",
    "start": "npm run browsersync & npm run build-demo"
  },
  "style": "index.sss",
  "author": {
    "name": "n-fuse GmbH",
    "email": "info@n-fuse.co",
    "url": "www.n-fuse.co"
  },
  "contributors": [
    {
      "name": "Thomas Hoppe",
      "email": "thomas.hoppe@n-fuse.co"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@vcl/build-demo": "0.3.2",
    "@vcl/container": "0.2.1",
    "@vcl/default-theme": "0.2.4",
    "@vcl/flex-layout": "0.2.2",
    "@vcl/preprocessor": "0.2.8",
    "@vcl/typography": "0.2.4",
    "browser-sync": "2.26.3",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.1",
    "merge2": "1.2.3",
    "normalize.css": "8.0.1",
    "pre-commit": "1.2.2"
  },
  "dependencies": {},
  "keywords": [
    "css",
    "vcl",
    "component",
    "default",
    "theme",
    "terms"
  ],
  "vcl": {
    "provides": [
      "theme-terms"
    ],
    "categories": [
      {
        "title": "Themes",
        "itemPriority": 20,
        "priority": 180
      }
    ]
  }
}
