  "private": true,
  "main": "package.json",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "git-init": "mkdir dist && cd dist && git init && git remote add origin git@github.com:vcl/vcl.github.io.git",
    "start": "npm-run-all clean install-modules gen-doc",
    "clean": "rm -rf node_modules package-lock.json",
    "install-modules": "npm install",
    "gen-doc": "vcl-doc-gen package.json dist --name=\"VCL Documentation\"",
    "git-push": "cd dist && git add -A . && git commit -a -m 'doc update' && git push --set-upstream origin master --force"
