{
  "name": "vcl-preprocessor-cli",
  "description": "VCL preprocessor cli tutorial",
  "scripts": {
    "build": "vcl-preprocessor vcl.sss vcl.css --watch",
    "serve": "browser-sync start --server --files vcl.css index.html",
    "start": "npm-run-all --parallel build serve"
  },
  "dependencies": {
    "@vcl/core-modules": "^0.4.0",
    "@vcl/font-awesome": "^0.4.0",
    "@vcl/theme": "^0.4.0",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@vcl/preprocessor": "^0.4.0",
    "browser-sync": "^2.26.3",
    "npm-run-all": "^4.1.5"
  }
}
